
{% extends 'base.html.twig' %}

{% block title %}Ajouter une participation{% endblock %}

{% block body %}
    <h1>Ajouter une participation</h1>
    {{ form_start(form) }}
        {{ form_row(form.nom) }}
        {{ form_row(form.prenom) }}
        {{ form_row(form.email) }}
        {{ form_row(form.user_id) }}
        {{ form_row(form.event_id) }}
        {{ form_row(form.camp_id) }}
        <button type="submit">Ajouter</button>
    {{ form_end(form) }}
{% endblock %}
